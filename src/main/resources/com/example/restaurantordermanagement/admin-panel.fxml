<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/17.0.12" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.restaurantordermanagement.controllers.LoginController">
   <children>
       <HBox fx:id="component" alignment="TOP_RIGHT" spacing="20.0">
           <Button mnemonicParsing="false" onAction="#onTabelMateButtonClick" text="Go to Tabel Mate">

               <opaqueInsets>
                   <Insets right="300.0" />
               </opaqueInsets></Button>
           
           <Button mnemonicParsing="false" onAction="#goBackFromButtonCick" text="Back">

               <opaqueInsets>
                   <Insets right="300.0" />
               </opaqueInsets></Button>

         <VBox.margin>
            <Insets />
         </VBox.margin>
         <padding>
            <Insets bottom="10.0" right="30.0" top="10.0" />
         </padding>
         <opaqueInsets>
            <Insets />
         </opaqueInsets>
       </HBox>

      <TabPane tabClosingPolicy="UNAVAILABLE" VBox.vgrow="ALWAYS">
          <tabs>
              <Tab text="Employees">
                  <content>
                      <fx:include source="employee-tab.fxml" />
                  </content>
              </Tab>

              <Tab text="Job">
                  <content>

                          <fx:include source="job-tab.fxml" />

                  </content>
              </Tab>
              <Tab text="Invoice">
                  <content>
                      <fx:include source="invoice-tab.fxml" />
                  </content>
              </Tab>
              <Tab text="Storage">
                  <content>
                      <fx:include source="storage-tab.fxml" />
                  </content>
              </Tab>
          </tabs>
      </TabPane>

   </children>
</VBox>
